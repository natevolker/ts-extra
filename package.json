{
  "name": "ts-extra",
  "version": "1.0.0",
  "description": "Extended utility types and functions for TypeScript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "build": "rm -rf dist; tsc",
    "build:docs": "typedoc",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "pre-push": "npm run lint; npm run typecheck; npm run test; npm run build:docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/natevolker/ts-extra.git"
  },
  "keywords": [
    "TypeScript",
    "Utility",
    "Types"
  ],
  "author": "Nate Volker <natevolker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/natevolker/ts-extra/issues"
  },
  "homepage": "https://github.com/natevolker/ts-extra#readme",
  "devDependencies": {
    "@microsoft/tsdoc": "^0.13.2",
    "@types/jest": "^27.0.2",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.24.2",
    "husky": "^7.0.2",
    "jest": "^27.2.4",
    "ts-jest": "^27.0.5",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "tsdoc": "0.0.4"
  }
}
